{
    "version": "0.2.0",
    "configurations": [
      {
        "type": "node",
        "request": "launch",
        "name": "Debug Vitest Tests",
        "program": "${workspaceFolder}/node_modules/vitest/vitest.mjs",
        "args": ["run", "--threads=false", "--no-coverage"],
        "autoAttachChildProcesses": true,
        "skipFiles": ["<node_internals>/**"],
        "sourceMaps": true,
        "env": {
          "NODE_ENV": "test"
        }
      }
    ]
  }
  
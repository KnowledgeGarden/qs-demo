{
  "name": "SenseCraft",
  "version": "0.0.1",
  "private": true,
  "description": "A Quasar Framework app",
  "author": "byroncoughlinsr <byroncoughlinsr@gmail.com>",
  "scripts": {
    "lint": "eslint --ext .js,.vue,.ts ./",
    "lint-staged": "lint-staged",
    "test": "echo \"See package.json => scripts for available tests.\" && exit 0",
    "prepare": "cd .. ; husky install",
    "dev": "quasar dev",
    "build": "quasar build",
    "test:scoring": "cucumber-js",
    "test:unit": "jest --updateSnapshot",
    "test:unit:ci": "jest --ci",
    "test:unit:coverage": "jest --coverage",
    "test:unit:watch": "jest --watch",
    "test:unit:watchAll": "jest --watchAll",
    "serve:test:coverage": "quasar serve test/jest/coverage/lcov-report/ --port 8788",
    "concurrently:dev:jest": "concurrently \"quasar dev\" \"jest --watch\""
  },
  "dependencies": {
    "@quasar/extras": "~1.12",
    "axios": "~0.25",
    "core-js": "^3.21",
    "grant": "~5.4",
    "luxon": "~2.3",
    "pg-hstore": "~2.3",
    "quasar": "~1.18",
    "socket.io-client": "~4.4",
    "uuid": "~8.3",
    "vue": "~2.6",
    "vue-class-component": "~7",
    "vuelidate": "^0.7.6",
    "vuex": "~3.6",
    "vuex-rest-api": "^2.14.0",
    "yaml": "^1.10.2"
  },
  "devDependencies": {
    "@cucumber/cucumber": "~7.3",
    "@cucumber/pretty-formatter": "^1.0.0-alpha.1",
    "@quasar/app": "~2.2.11",
    "@quasar/quasar-app-extension-testing": "~1.0.5",
    "@quasar/quasar-app-extension-testing-unit-jest": "~2.2.3",
    "@tsconfig/recommended": "~1.0",
    "@types/chai": "~4.3",
    "@typescript-eslint/eslint-plugin": "~5.10",
    "@typescript-eslint/parser": "~5.10",
    "@vue/eslint-config-typescript": "~10",
    "@vue/test-utils": "^1.3.0",
    "babel-eslint": "^10.0.1",
    "chai": "~4.3.6",
    "cucumber-tsflow": "^4.0.0-preview.7",
    "eslint": "~8.8",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-cucumber": "~1.4",
    "eslint-plugin-jest": "~26",
    "eslint-plugin-markdown": "~2.2",
    "eslint-plugin-vue": "~8",
    "eslint-webpack-plugin": "~2.6",
    "husky": "~7.0",
    "jest-matcher-vue-test-utils": "^2.0.1",
    "jest-mock-axios": "^4.5.0",
    "lint-staged": "~12.3",
    "prettier": "~2.5",
    "ts-node": "~10.4",
    "typescript": "~4.5",
    "vue-cli-plugin-store": "~0.0.7",
    "vue-jest": "^3.0.7",
    "vuex-mock-store": "^0.0.8"
  },
  "browserslist": [
    "last 10 Chrome versions",
    "last 10 Firefox versions",
    "last 4 Edge versions",
    "last 7 Safari versions",
    "last 8 Android versions",
    "last 8 ChromeAndroid versions",
    "last 8 FirefoxAndroid versions",
    "last 10 iOS versions",
    "last 5 Opera versions"
  ],
  "engines": {
    "node": ">= 16",
    "npm": ">= 8"
  },
  "lint-staged": {
    "**/*": [
      "eslint",
      "prettier --write --ignore-unknown"
    ]
  },
  "productName": "SenseCraft",
  "resolutions": {
    "axios": "~0.25"
  }
}

{
  "name": "SenseCraft",
  "version": "0.0.1",
  "private": true,
  "description": "A Collective Sensemaking Game",
  "author": "byroncoughlinsr <byroncoughlinsr@gmail.com>",
  "scripts": {
    "lint": "eslint --ext .js,.vue,.ts ./src ./features ./test",
    "lint-staged": "lint-staged",
    "test:integration": "cucumber-js -t @integration",
    "test": "echo \"See package.json => scripts for available tests.\" && exit 0",
    "prepare": "cd .. ; husky install",
    "dev": "quasar dev",
    "build": "quasar build",
    "test:scoring": "cucumber-js -t @scoring",
    "test:unit": "jest --updateSnapshot",
    "test:unit:ci": "jest --ci",
    "test:unit:coverage": "jest --coverage",
    "test:unit:watch": "jest --watch",
    "test:unit:watchAll": "jest --watchAll",
    "serve:test:coverage": "quasar serve test/jest/coverage/lcov-report/ --port 8788",
    "concurrently:dev:jest": "concurrently \"quasar dev\" \"jest --watch\""
  },
  "dependencies": {
    "@quasar/extras": "~1.16",
    "axios": "~1.6",
    "core-js": "^3",
    "jws": "~4.0",
    "luxon": "~3.4",
    "pg-hstore": "~2.3",
    "quasar": "legacy",
    "robust-websocket": "~1.0",
    "uuid": "~9",
    "vue": "~2.7",
    "vue-class-component": "~7",
    "vuelidate": "^0.7.6",
    "vuex": "~3.6",
    "vuex-rest-api": "^2.15",
    "yaml": "~2.3"
  },
  "devDependencies": {
    "@cucumber/cucumber": "~10.0",
    "@cucumber/pretty-formatter": "^1.0",
    "@quasar/app": "~2.4",
    "@quasar/quasar-app-extension-testing": "~1.0",
    "@quasar/quasar-app-extension-testing-unit-jest": "~2.2",
    "@tsconfig/recommended": "~1.0",
    "@types/chai": "~4.3",
    "@types/node": "~18",
    "@typescript-eslint/eslint-plugin": "~6.9",
    "@typescript-eslint/parser": "~6.9",
    "@vue/eslint-config-typescript": "~11",
    "@vue/test-utils": "~1.3",
    "babel-eslint": "~10.1",
    "chai": "~4.3",
    "cucumber-tsflow": "~4.2",
    "eslint": "~8",
    "eslint-config-prettier": "~9.0",
    "eslint-plugin-cucumber": "~2.0",
    "eslint-plugin-jest": "~27",
    "eslint-plugin-markdown": "~3",
    "eslint-plugin-vue": "~9",
    "eslint-webpack-plugin": "~2",
    "husky": "~8.0",
    "jest-environment-jsdom": "~29",
    "jest-matcher-vue-test-utils": "~2.0",
    "jest-mock-axios": "^4.7",
    "libqp": "~2.0",
    "lint-staged": "~15.0",
    "prettier": "~3.0",
    "selenium-webdriver": "~4.15",
    "ts-jest": "~29",
    "ts-node": "TypeStrong/ts-node#main",
    "typescript": "~5.2",
    "vue-cli-plugin-component": "~1.12.2",
    "vue-cli-plugin-components": "~0.0.0",
    "vue-cli-plugin-store": "~0.0.7",
    "vue-jest": "^3.0.7",
    "vuex-mock-store": "^0.1.0"
  },
  "browserslist": [
    "last 10 Chrome versions",
    "last 10 Firefox versions",
    "last 4 Edge versions",
    "last 7 Safari versions",
    "last 8 Android versions",
    "last 8 ChromeAndroid versions",
    "last 8 FirefoxAndroid versions",
    "last 10 iOS versions",
    "last 5 Opera versions"
  ],
  "engines": {
    "node": ">= 18",
    "npm": ">= 9"
  },
  "lint-staged": {
    "**/*": [
      "eslint",
      "prettier --write --ignore-unknown"
    ]
  },
  "productName": "SenseCraft",
  "resolutions": {
    "@typescript-eslint/parser": "~6.9",
    "@typescript-eslint/eslint-plugin": "~6.9",
    "axios": "~1.6"
  },
  "overrides": {
    "@typescript-eslint/parser": "~6.9",
    "@typescript-eslint/eslint-plugin": "~6.9",
    "axios": "~1.6"
  }
}
